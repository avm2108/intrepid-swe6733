{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.0.0",
    "axios": "^1.3.5",
    "eslint-plugin-jest-dom": "^4.0.3",
    "intrepid-swe6733": "file:..",
    "jest-junit": "^15.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.4.0",
    "react-modal": "^3.16.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3",
    "reselect": "^4.1.7",
    "styled-components": "^5.3.9",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose",
    "testNoInteractive": "react-scripts test --verbose --watchAll=false",
    "coverage": "react-scripts test --verbose --coverage --watchAll --collectCoverageFrom='src/pages/**/*.{js,jsx}' ----collectCoverageFrom='src/pages/**/*.{types,stories,constants,test,spec}.{js,jsx}'",
    "coverageNoInteractive": "react-scripts test --coverage --verbose --watchAll=false",
    "test-CI": "react-scripts test --verbose --ci --runInBand --reporters=default --reporters=jest-junit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:jest-dom/recommended"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "statements": 80
      }
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!axios).+\\.js$"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
